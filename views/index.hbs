<div class="container">
    <h1>{{title}}</h1>
    <ul>
        {{#each qs}}
        <div class="row">
            <div class="col-6">
                <li><a href="/question/get/{{this.id}}">{{this.question}}</a></li>
            </div>
            <div class="col-3">
                <span>{{this.answer}}</span>
            </div>
            <div class="col-2">
                <span>{{formatDate this.date}}</span>
            </div>
        </div>
        {{/each}}
    </ul>
</div>
